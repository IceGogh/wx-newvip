.common-header {
  border: none;
  color: #fff;
  position: relative;
  z-index: 10;
  background: transparent;
}

.common-header .backBtn{
  background-image: url(https://match.steampp.com/mobile/static/wx/account/arrowLeft2.png);
}

.headerBg {
  width: 100%;
  height: 208rpx;
  position: absolute;
  top: 0;
  left: 0;
  background: url(https://match.steampp.com/mobile/static/wx/roll/create1Top.png) no-repeat center center;
  background-size: cover;
}

.roomInfo {
  width: 92%;
  height: 192rpx;
  border-radius: 27rpx;
  background: #fff;
  position: relative;
  z-index: 20;
  margin: 20rpx 4%;
  box-shadow: 0 0 10rpx 10rpx rgba(10, 10, 10, .1);
}

.photo {
  width: 90%;
  height: 118rpx;
  margin: 0 auto;
  position: relative;
}

.photo image {
  width: 90rpx;
  height: 90rpx;
  border-radius: 50%;
  position: absolute;
  top: 14rpx;
  left: 10rpx;
}

.photo > view:nth-child(2) {
  padding: 0 12rpx;
  height: 90rpx;
  line-height: 90rpx;
  position: absolute;
  top: 14rpx;
  left: 120rpx;
}

.link {
  width: 100%;
  height: 72rpx;
}

.link > view {
  width: 50%;
  height: 72rpx;
  line-height: 72rpx;
  text-align: center;
  float: left;
  color: #9da0a5;
  border-top: 2rpx solid #edf2f5;
  font-size: 28rpx;
}

.link .tv{
  background: url(https://match.steampp.com/mobile/static/wx/roll/tv.png) no-repeat 100rpx center;
  background-size: 32rpx;
  position: relative;
}

.tv::after {
  content: '';
  position: absolute;
  right: 0;
  top: 18rpx;
  height: 36rpx;
  width: 2rpx;
  background: #edf2f5;
}

.link .weibo{
  background: url(https://match.steampp.com/mobile/static/wx/roll/weibo.png) no-repeat 100rpx center;
  background-size: 32rpx;
}

.timeRemain {
  width: 94%;
  margin: 20rpx auto 0;
  height: 108rpx;
  position: relative;
}

.timeRemain .in .fontColor3{
  font-size: 26rpx;
  height: 34rpx;
  line-height: 34rpx;
}

.timeRemain .in .fontColor3{
  font-size: 30rpx;
  height: 50rpx;
  line-height: 50rpx;
}

.timeRemain .btn {
  position: absolute;
  right: 0;
  top: 0;
  height: 108rpx;
}

.timeRemain .btn > view {
  width: 166rpx;
  height: 56rpx;
  line-height: 56rpx;
  text-align: center;
  color: #fff;
  border-radius: 28rpx;
  float: left;
  margin: 26rpx 20rpx 0;
  font-size: 28rpx;
}

.timeRemain  .btn .changeInfo {
  background: #5cb0f1;
}

.timeRemain .btn  .rightNow{
  background: #ffc63f;
  margin-right: 0;
}

.timeRemain .btn  .closed{
  background: #bbc5cd;
  margin-right: 0;
}

.note {
  width: 100%;
  border-top: 15rpx solid #edf2f5;
  border-bottom: 15rpx solid #edf2f5;
  padding: 10rpx 0 24rpx;
}

.note .title {
  width: 94%;
  margin: 0 auto;
  height: 62rpx;
  line-height: 62rpx;
  font-size: 30rpx;
}

.note .in {
  width: 94%;
  margin: 0 auto;
  line-height: 36rpx;
  color: #9da0a5;
  font-size: 26rpx;
}

.propsBox {
  padding: 10rpx 0;
}

.propsBox .title {
  width: 94%;
  margin: 0 auto;
  height: 69rpx;
  line-height: 69rpx;
  font-size: 30rpx;
  position: relative;
}

.propsBox .title > view {
  position: absolute;
  top: 0;
  right: 0;
  line-height: 69rpx;
  height: 69rpx;
  text-align: right;
}

.propsBox span {
  color: #f1883e;
}

.propsBox .propList {
  width: 92%;
  height: 240rpx;
  margin: 6rpx auto;
}

.propsBox .propList > view {
  width: 159rpx;
  height: 107rpx;
  float: left;
  margin-right: 18rpx;
  margin-bottom: 16rpx;
}

.propsBox .propList > view:nth-of-type(4n) {
  margin-right: 0;
}

.propsBox .propList > view image {
  width: 100%;
  height: 100%;
  float: left;
}

.loadMore {
  width: 41rpx;
  height: 41rpx;
  margin: 0 auto;
  background: url(https://match.steampp.com/mobile/static/wx/roll/DbArrowBtm.png) no-repeat center center;
  background-size: 30rpx;
}

.winnerListBox {
  width: 94%;
  margin: 0 auto;
}

.winnerListBox .winnerInfo {
  padding: 18rpx 0 8rpx;
  border-top: 2rpx solid #edf2f5;
}

.winnerListBox .winnerInfo:nth-of-type(1){
  border-top: none;
}

.winnerListBox .winnerInfo .photo {
  width: 90rpx;
  height: 90rpx;
  float: left;
}

.winnerListBox .winnerInfo .photo image {
  width: 100%;
  height: 100%;
  border-radius: 50%;  
}

.winnerListBox .winnerInfo .winProps {
  float: right;
  width: 570rpx;
}

.winnerListBox  .winProps .title {
  width: 100%;
  height: 48rpx;
  line-height: 48rpx;
  position: relative;
  margin-bottom: 10rpx;
}

.winnerListBox  .winProps .title > view:nth-child(2){
  position: absolute;
  right: 18rpx;
  top: 0;
  text-align: right;
  color: #ee6a0e;
}

.winnerListBox  .winProps .props {
  width: 125rpx;
  height: 83rpx;
  float: left;
  margin-right: 17rpx;
  margin-bottom: 11rpx;
}

.winnerListBox  .winProps .props :nth-of-type(4n){
  margin-right: 0;
}

.winnerListBox  .winProps .props  image{
  width: 100%;
  height: 100%;
  float: left;
}

.joinedList {
  width: 94%;
  margin: 0 auto;
}

.joinedList > view {
  width: 84rpx;
  float: left;
  margin-right: 40rpx;
  margin-bottom: 20rpx;
}
.joinedList > view:nth-of-type(6n){
  margin-right: 0;
}

.joinedList > view image {
  width: 84rpx; 
  height: 84rpx;
  border-radius: 50%;
  float: left;
}

.joinedList > view view {
  width: 84rpx;
  height: 51rpx;
  line-height: 51rpx;
  color: #9da0a5;
  font-size: 28rpx;
}

.changeRoomInfo {
  position: fixed;
  top: 0;
  right: 0;
  width: 100%;
  height: 100%;
  background: rgba(10, 10, 10, .4);
  z-index: 100;
}

.changeRoomInfo .in {
  width: 94%;
  position: absolute;
  top: 116rpx;
  left: 3%;
  background: #fff;
  border-radius: 14rpx;
}

.changeRoomInfo .in .close {
  position: absolute;
  right: -10rpx;
  top: 20rpx;
  width: 41rpx;
  height: 41rpx;
  background: url(https://match.steampp.com/mobile/static/wx/account/close.png) no-repeat center center;
  background-size: 30rpx;
}

.changeRoomInfo .in .lin {
  height: 104rpx;
  line-height: 104rpx;
  text-indent: 30rpx;
  font-size: 30rpx;
  color: #888a90;
  border-top: 2rpx #edf2f5 solid;
  position: relative;
}

.changeRoomInfo .in .lin input{
  position: absolute;
  right: 30rpx;
  top: 0;
  height: 104rpx;
  color: #9da0a5;
  text-align: right;
}

.changeRoomInfo .in .confirm {
  width: 100%;
  height: 154rpx;
  border-top: 2rpx #edf2f5 solid;
}
.changeRoomInfo .in .confirm > view {
  width: 236rpx;
  height: 69rpx;
  line-height: 69rpx;
  border-radius: 35rpx;
  text-align: center;
  color: #fff;
  background: #ffc63f;
  position: relative;
  top: 42rpx;
  margin: 0 auto;
}
