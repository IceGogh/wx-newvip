<view class="common-header">
    <view class='backBtn' bindtap='goBack'></view>
    房间号: {{roomNub}}
  </view>
  <view class='headerBg'></view>

  <view class='roomInfo'>
    <view class='photo'>
      <image src="https://vip-account.oss-cn-hangzhou.aliyuncs.com/files/171009/X00068F9.jpg">头像</image>
      <view>heytoo</view>
    </view>
    <view class='link'>
      <view class='tv'>直播</view>
      <view class='weibo'>微博</view>
    </view>
</view>

<view class='timeRemain'>
  <view class='in'>
    <view class='fontColor3'>截止时间</view>
    <view class='fontColor2'>{{timeRemain}}11111</view>
  </view>
  <view class='btn'>
    <view class='changeInfo' bindtap='openChangeRoomInfo'>修改信息</view>
    <view class='rightNow'>立即开奖</view>
    <view class='closed'>已结束</view>
  </view>
</view>

<view class='note'>
  <view class='title'>ROLL说明</view>
  <view class='in'>{{note}}ROLL键盘的密码在我微博里面，大家关注我的微博会有不定期的ROLL奖品活动</view>
</view>

