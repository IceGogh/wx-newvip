<!-- 修改房间信息  -->
<view class='changeRoomInfo' hidden='{{!changeRoomInfo}}'>
  <view class='in'>
    <view class='photo'>
      <image src="https://vip-account.oss-cn-hangzhou.aliyuncs.com/files/171009/X00068F9.jpg">头像</image>
      <view>heytoo</view>
      <view class='close' bindtap='closeChangeRoomInfo'></view>
    </view>

    <view class='lin'>
      中奖人数
      <input type='number' placeholder='修改中奖人数' ></input>
    </view>
    <view class='lin'>
      房间密码
      <input type='text'  placeholder='修改房间密码' ></input>
    </view>
    <view class='confirm'>
      <view>确认修改</view>
    </view>
  </view>
</view>