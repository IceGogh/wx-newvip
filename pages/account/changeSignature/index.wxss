.keyin {
  width: 94%;
  height: 60rpx;
  line-height: 60rpx;
  border-bottom: 1rpx solid #ffc63f;
  position: relative;
  margin: 56rpx auto;
  font-size: 32rpx;
}

input {
  padding-left: 10rpx;
  width: 70%;
}

.close {
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  top: 0;
  right: 0;
  background: url(https://match.steampp.com/mobile/static/wx/account/close.png) no-repeat center center;
  background-size: 34rpx;
}

.limit{
  width: 110rpx;
  height: 60rpx;
  line-height: 60rpx;
  position: absolute;
  bottom: -60rpx;
  right: 0;
  font-size: 32rpx;
}
